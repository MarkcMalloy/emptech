# Import the PaperDetector class from the 'CV' module
from paper_detector import PaperDetector

# Create an instance of the PaperDetector class
paper_detector = PaperDetector()

# Call the find_a4_contour() method with an image path as an argument
image_path = "../assets/antonio(1).jpg"  # Replace with your actual image file path
paper_detector.find_a4_contour(image_path)

